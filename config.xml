<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gburis.lista.casamento" version="2.5.06" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>A e G</name>
    <description>
        Meu Casamento
    </description>
    <author email="gabrielleiteolimpio@gmail.com" href="http://gburis.com.br">Gburis TEC</author>
    <content src="index.html" />
    <preference name="Fullscreen" value="true" />
    <preference name="BackgroundColor" value="0xfffffff1"/>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <icon src="www/img/logo.jpeg" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
